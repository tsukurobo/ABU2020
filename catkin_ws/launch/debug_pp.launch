<launch>
	<node pkg="pr_pick_pass" name="pick_pass_node" type="pick_pass_node">
		<param name="FREQ" value="25"/>
		<param name="POW_LOWER" value="30"/>
		<param name="POW_RAISE" value="-30"/>
		<param name="POW_WIND" value="30"/>
		<param name="DEG_1" value="180"/>
		<param name="DEG_2" value="10"/>
